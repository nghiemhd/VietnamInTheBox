                <component
                  refId="Package\Order Import\Import Order\OLE DB Source"
                  name="OLE DB Source"
                  componentClassID="{165A526D-D5DE-47FF-96A6-F8274C19826B}"
                  description="OLE DB Source"
                  usesDispositions="true"
                  version="7"
                  contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7">
                  <properties>
                    <property
                      name="CommandTimeout"
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out.">0</property>
                    <property
                      name="OpenRowset"
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."></property>
                    <property
                      name="OpenRowsetVariable"
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."></property>
                    <property
                      name="SqlCommand"
                      dataType="System.String"
                      description="The SQL command to be executed."
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT
	o.ChanelId,
	o.SourceId,
	ISNULL(o.ReturnReasonId,0) AS ReturnReasonId,
	o.OrderNo,
	o.OrderDate,
	b.ProductId,
	CASE WHEN pvd.Name IS NULL THEN
		(CASE WHEN 
			bp.Name LIKE N'%Ná»¯%' 
			OR bp.Name LIKE N'%Äáº§m%' 
			OR bp.Name LIKE N'%Son%' 
			OR bp.Name LIKE N'%Hai dÃ¢y%' 
			OR bp.Name LIKE N'%Káº» máº¯t%'
			OR bp.Name LIKE N'%Pháº¥n%'
			OR bp.Name LIKE N'%MÃ³ng%'
		THEN N'Ná»¯'
		WHEN bp.Name LIKE N'%Nam%' THEN N'Name' ELSE 'NULL'
		END)
	ELSE pvd.NAME END AS Sex, 
	CASE WHEN LEN(REPLACE(RTRIM(LTRIM(Mobile)),'.','')) &gt; 6 THEN REPLACE(RTRIM(LTRIM(Mobile)),'.','')
	ELSE '00000000000'END AS CustomerId,
	ISNULL(pv.Name,'NULL') AS Size,	
    SUM(d.Qty) AS Qty,
	SUM(d.SellUnitPrice * d.Qty) AS SubAmount,
	SUM((d.SellUnitPrice * d.Qty) - d.LineAmount) AS LineDiscount,
	CASE WHEN o.SubTotal &lt;&gt; 0 THEN ((o.Discount / o.SubTotal) * SUM(d.LineAmount)) ELSE 0 END AS OrderDiscount,
	CASE WHEN o.SubTotal &lt;&gt; 0 THEN ((o.ShippingFee / o.SubTotal) * SUM(d.LineAmount)) ELSE 0 END AS PVC,
	CASE WHEN o.SubTotal &lt;&gt; 0 THEN ((o.AeroShippingFee / o.SubTotal) * SUM(d.LineAmount)) ELSE 0 END AS APVC,
	SUM(d.SellUnitPrice * d.Qty) --subamount
		- SUM((d.SellUnitPrice * d.Qty) - d.LineAmount) -- linediscount
		- CASE WHEN o.SubTotal &lt;&gt; 0 THEN ((o.Discount / o.SubTotal) * SUM(d.LineAmount)) ELSE 0 END -- orderdiscount
		+ CASE WHEN o.SubTotal &lt;&gt; 0 THEN ((o.ShippingFee / o.SubTotal) * SUM(d.LineAmount)) ELSE 0 END -- pvc
		- CASE WHEN o.SubTotal &lt;&gt; 0 THEN ((o.AeroShippingFee / o.SubTotal) * SUM(d.LineAmount)) ELSE 0 END -- apvc
		 AS Amount,
	-SUM(ivm.CostAmount) AS Cost

FROM dbo.POS_SaleOrder o
INNER JOIN dbo.POS_SaleOrderDetail d ON o.Id = d.SaleOrderId
INNER JOIN dbo.IVM_Barcode b ON d.BarcodeId = b.Id
INNER JOIN dbo.Product bp ON b.ProductId = bp.Id
INNER JOIN dbo.IVM_Transaction ivm ON b.Id = ivm.BarcodeId AND ivm.RefId = o.Id AND TypeId = 2
LEFT JOIN (SELECT
	db.BarcodeId,
	pv.Name
	From dbo.IVM_BarcodeDetail db
