                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT
CustomerId,
CustomerName
FROM
(
SELECT
ROW_NUMBER() OVER (PARTITION BY O.Mobile ORDER BY LEN(ISNULL(CustomerName,'NONAME')) DESC) Seq,
CASE WHEN LEN(O.Mobile) &gt; 6 THEN O.Mobile
ELSE '00000000000' END AS CustomerId,
ISNULL(CustomerName,'NONAME') AS CustomerName,
LEN(ISNULL(CustomerName,'NONAME')) AS CustomerNameLen
FROM
	(SELECT
	REPLACE(RTRIM(LTRIM(Mobile)),'.','') AS Mobile,
	CASE WHEN LEN(CustomerName) &lt; 1 THEN NULL ELSE LTRIM(RTRIM(CustomerName)) END AS CustomerName
	FROM dbo.POS_SaleOrder) O
	) T

WHERE Seq = 1 AND CustomerId &lt;&gt; '00000000000'

UNION ALL

SELECT '00000000000' AS CustomerId, 'NULLNAME' AS CustomerName</property>
