_IDENTIFIER ON&#xA;GO&#xA;&#xA;CREATE TABLE [dbo].[DimSex](&#xA;&#x9;[Sex] [nvarchar](50) NOT NULL,&#xA; CONSTRAINT [PK_DimSex] PRIMARY KEY CLUSTERED &#xA;(&#xA;&#x9;[Sex] ASC&#xA;)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON) ON [PRIMARY]&#xA;) ON [PRIMARY]&#xA;&#xA;GO&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;/****** Object:  UserDefinedFunction [dbo].[ufn_AddZeroBefore]    Script Date: 5/25/2014 3:06:28 PM ******/&#xA;SET ANSI_NULLS ON&#xA;GO&#xA;SET QUOTED_IDENTIFIER ON&#xA;GO&#xA;&#xA;CREATE FUNCTION [dbo].[ufn_AddZeroBefore]&#xA;(&#xA;&#x9;@num INT,&#xA;&#x9;@len int&#xA;)&#xA;RETURNS VARCHAR(MAX)&#xA;AS&#xA;BEGIN&#xA;&#xA;&#x9;DECLARE @currLen INT&#xA;&#x9;DECLARE @temp VARCHAR(MAX)&#xA;&#xA;&#x9;set @temp = CAST(@num AS VARCHAR(MAX))&#xA;&#x9;SET @currLen = LEN(@temp)&#xA;&#xA;&#x9;IF @currLen &lt; @len&#xA;&#x9;BEGIN&#xA;&#x9;&#x9;WHILE @currLen &lt; @len&#xA;&#x9;&#x9;BEGIN&#xA;&#x9;&#x9;&#x9;SET @temp = '0' + @temp&#xA;&#x9;&#x9;&#x9;set @currLen = @currLen + 1 &#xA;&#x9;&#x9;END&#xA;&#x9;END&#xA;&#x9;&#xA;&#x9;RETURN @temp&#xA;&#x9;&#xA;END&#xA;&#xA;GO&#xA;/****** Object:  UserDefinedFunction [dbo].[ufn_DateToDateKey]    Script Date: 5/25/2014 3:06:28 PM ******/&#xA;SET ANSI_NULLS ON&#xA;GO&#xA;SET QUOTED_IDENTIFIER ON&#xA;GO&#xA;CREATE FUNCTION [dbo].[ufn_DateToDateKey]&#xA;(&#xA;&#x9;@date DATETIME&#xA;)&#xA;RETURNS int&#xA;AS&#xA;BEGIN&#xA;&#x9;RETURN YEAR(@date) * 10000 + MONTH(@date) * 100 + DAY(@date)&#xA;END&#xA;&#xA;GO&#xA;/****** Object:  UserDefinedFunction [dbo].[ufn_GenWeek]    Script Date: 5/25/2014 3:06:28 PM ******/&#xA;SET ANSI_NULLS ON&#xA;GO&#xA;SET QUOTED_IDENTIFIER ON&#xA;GO&#xA;CREATE FUNCTION [dbo].[ufn_GenWeek]&#xA;(&#xA;&#x9;@startYear int,&#xA;&#x9;@endYear INT,&#xA;&#x9;@dayOfStart VARCHAR(MAX)&#xA;)&#xA;RETURNS @ResultTable table(&#xA;&#x9;&#x9;[DateKey] INT PRIMARY KEY NOT NULL,&#xA;&#x9;&#x9;[Week] CHAR(6)&#xA;&#x9;&#x9;)&#xA;AS &#xA;BEGIN&#xA;&#x9;&#x9;&#xA;&#xA;&#x9;DECLARE @startDateOfWeek DATETIME&#xA;&#x9;DECLARE @startDateOfWeekNextYear DATETIME&#xA;&#xA;&#x9;SET @startDateOfWeek = CAST(@startYear AS CHAR(4)) + '-1-1' &#xA;&#x9;SET @startDateOfWeekNextYear = CAST(@endYear AS CHAR(4)) + '-1-1' &#xA;&#xA;&#x9;WHILE DATENAME(dw,@startDateOfWeek) &lt;&gt; @dayOfStart &#xA;&#x9;BEGIN&#xA;&#x9;&#x9;Set @startDateOfWeek = DATEADD(DAY,1,@startDateOfWeek)&#xA;&#x9;END&#xA;&#xA;&#x9;--PRINT @startDateOfWeek&#xA;&#xA;&#x9;WHILE DATENAME(dw,@startDateOfWeekNextYear) &lt;&gt; @dayOfStart &#xA;&#x9;BEGIN&#xA;&#x9;&#x9;Set @startDateOfWeekNextYear = DATEADD(DAY,1,@startDateOfWeekNextYear)&#xA;&#x9;END&#xA;&#xA;&#x9;--PRINT @startDateOfWeekNextYear&#xA;&#xA;&#x9;DECLARE @weekCount INT&#xA;&#x9;DECLARE @currentDate DATETIME&#xA;&#xA;&#x9;SET @currentDate = @startDateOfWeek&#xA;&#xA;&#x9;SET @weekCount = 0&#xA;&#xA;&#x9;DECLARE @currMonth AS VARCHAR(2)&#xA;&#x9;DECLARE @newMonth AS VARCHAR(2)&#xA;&#x9;DECLARE @monthCount AS INT&#xA;&#x9;DECLARE @dayCount AS INT&#xA;&#x9;SET @monthCount = 0&#xA;&#x9;&#xA;&#x9;DECLARE @currYear INT&#xA;&#x9;DECLARE @newYear INT&#xA;&#xA;&#x9;WHILE @currentDate &lt; @startDateOfWeekNextYear&#xA;&#x9;BEGIN&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;set @newYear = DATEPART(YEAR,@currentDate)&#xA;&#x9;&#x9;IF @newYear &lt;&gt; @currYear OR @currYear IS NULL&#xA;&#x9;&#x9;BEGIN&#xA;&#x9;&#x9;&#x9;SET @currYear = @newYear&#xA;&#x9;&#x9;&#x9;SET @weekCount = 1&#xA;&#x9;&#x9;END&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;SET @newMonth = CAST(DATEPART(month,@currentDate) AS VARCHAR(2))&#xA;&#x9;&#x9;IF @currMonth IS NULL&#xA;&#x9;&#x9;&#x9;SET @currMonth = @newMonth&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;IF @newMonth &lt;&gt; @currMonth&#xA;&#x9;&#x9;BEGIN&#xA;&#x9;&#x9;&#x9;SET @monthCount = 1&#xA;&#x9;&#x9;&#x9;SET @currMonth = @newMonth&#x9;&#xA;&#x9;&#x9;END&#xA;&#x9;&#x9;ELSE &#xA;&#x9;&#x9;&#x9;SET @monthCount = @monthCount + 1&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;SET @dayCount = 0&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;WHILE @dayCount &lt; 7&#xA;&#x9;&#x9;BEGIN&#xA;&#x9;&#x9;&#x9;INSERT INTO @ResultTable&#xA;&#x9;&#x9;&#x9;SELECT dbo.ufn_DateToDateKey(@currentDate),'T' + SUBSTRING(CAST(@currYear AS CHAR(4)),3,2) + '_' + dbo.ufn_AddZeroBefore(@weekCount,2)&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;set @dayCount = @dayCount + 1&#xA;&#x9;&#x9;&#x9;Set @currentDate = DATEADD(DAY,1,@currentDate)&#xA;&#x9;&#x9;END&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;--Set @currentDate = DATEADD(DAY,1,@currentDate)&#xA;&#x9;&#x9;Set @weekCount = @weekCount + 1&#xA;&#x9;END&#xA;&#x9;&#xA;&#x9;RETURN&#xA;&#xA;END&#xA;&#xA;&#xA;&#xA;&#xA;GO&#xA;/****** Object:  UserDefinedFunction [dbo].[ufn_IsDayOff]    Script Date: 5/25/2014 3:06:28 PM ******/&#xA;SET ANSI_NULLS ON&#xA;GO&#xA;SET QUOTED_IDENTIFIER ON&#xA;GO&#xA;CREATE FUNCTION [dbo].[ufn_IsDayOff]&#xA;(&#xA;&#x9;@date DATETIME&#xA;)&#xA;/*&#xA;select dbo.[ufn_IsDayOff]('2013-11-23')&#xA;*/&#xA;RETURNS BIT&#xA;BEGIN&#xA;&#x9;--Check 7,8&#xA;&#x9;DECLARE @tmp AS varchar(100)&#xA;&#x9;SET @tmp = DATENAME(weekday,@date)&#xA;&#x9;&#xA;&#x9;IF @tmp = 'Sunday' OR @tmp = 'Saturday'&#xA;&#x9;BEGIN&#xA;&#x9;&#x9;RETURN 1&#xA;&#x9;END&#xA;&#x9;&#xA;&#x9;RETURN 0&#xA;END&#xA;&#xA;&#xA;GO&#xA;/****** Object:  UserDefinedFunction [dbo].[ufn_ToWeekDayVN]    Script Date: 5/25/2014 3:06:28 PM ******/&#xA;SET ANSI_NULLS ON&#xA;GO&#xA;SET QUOTED_IDENTIFIER ON&#xA;GO&#xA;CREATE FUNCTION [dbo].[ufn_ToWeekDayVN]&#xA;(&#xA;&#x9;@usName AS NVARCHAR(MAX)&#xA;)&#xA;RETURNS NVARCHAR(MAX)&#xA;AS&#xA;BEGIN&#xA;&#xA;&#x9;RETURN CASE @usName WHEN N'Monday' THEN N'Thá»© 2'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN N'Tuesday' THEN N'Thá»© 3'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN N'Wednesday' THEN N'Thá»© 4'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN N'Thursday' THEN N'Thá»© 5'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN N'Friday' THEN N'Thá»© 6'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN N'Saturday' THEN N'Thá»© 7'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN N'Sunday' THEN N'Chá»§ nháº­t'&#xA;&#x9;&#x9;   END&#x9;&#xA;END&#xA;&#xA;GO&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;/****** Object:  StoredProcedure [dbo].[usp_GenDimDate]    Script Date: 5/25/2014 3:10:03 PM ******/&#xA;SET ANSI_NULLS ON&#xA;GO&#xA;SET QUOTED_IDENTIFIER ON&#xA;GO&#xA;&#xA;CREATE PROCEDURE [dbo].[usp_GenDimDate]&#xA;(&#xA;&#x9;@startYear int,&#xA;&#x9;@endYear INT,&#xA;&#x9;@dayOfStart VARCHAR(MAX)&#xA;)&#xA;AS&#xA;&#xA;/*&#xA;* exec usp_GenDimDate 2001,2013&#xA;*/&#xA;&#xA;DECLARE @startDate AS DATETIME&#xA;DECLARE @dateKey AS int&#xA;&#xA;SET @startDate = CAST(@startYear AS CHAR(4)) + '-1-1' &#xA;&#xA;WHILE YEAR(@startDate) &lt;= @endYear&#xA;BEGIN&#xA;&#x9;&#xA;&#x9;set @dateKey = dbo.[ufn_DateToDateKey](@startDate)&#xA;&#x9;&#xA;&#x9;INSERT INTO [DimDate]&#xA;           ([DateKey]&#xA;           ,[Year]&#xA;           ,[Quarter]&#xA;           ,[Month]&#xA;           ,[Day]&#xA;           ,[DayOfWeek]&#xA;           ,[DateString]&#xA;           ,IsDayOff&#xA;           )&#xA;     VALUES&#xA;           (@dateKey&#xA;           ,CAST(YEAR(@startDate) AS CHAR(4))&#xA;           ,'QuÃ½ ' + CAST(DATEPART(quarter,@startDate) AS char(1))&#xA;           ,'ThÃ¡ng ' + dbo.ufn_AddZeroBefore(DATEPART(MONTH,@startDate),2)&#xA;           ,dbo.ufn_AddZeroBefore(DATEPART(DAY,@startDate),2)&#xA;           ,dbo.[ufn_ToWeekDayVN](DATENAME(weekday,@startDate))&#xA;           ,dbo.ufn_AddZeroBefore(DATEPART(DAY,@startDate),2) + '/' + dbo.ufn_AddZeroBefore(DATEPART(MONTH,@startDate),2) + '/' + CAST(YEAR(@startDate) AS CHAR(4))&#xA;           ,dbo.ufn_IsDayOff(@startDate)&#xA;           )&#xA;&#x9;&#xA;&#x9;set @startDate = DATEADD(DAY,1,@startDate)&#xA;END&#xA;&#xA;UPDATE DimDate&#xA;SET&#xA;&#x9;[Week] = b.[Week]&#xA;FROM DimDate dd INNER JOIN dbo.ufn_GenWeek(@startYear-1,@endYear+1,@dayOfStart) b&#xA;&#x9;ON dd.DateKey = b.DateKey&#xA;&#xA;&#xA;INSERT INTO [DimDate]&#xA;           ([DateKey]&#xA;           ,[Year]&#xA;           ,[Quarter]&#xA;           ,[Month]&#xA;           ,[Day]&#xA;           ,[DayOfWeek],[Week],[DateString],IsDayOff)&#xA;     VALUES&#xA;           (10000000&#xA;           ,'0000'&#xA;           ,'QuÃ½ 0'&#xA;           ,'ThÃ¡ng 00'&#xA;           ,'00'&#xA;           ,'00','T00_00','00/00/0000',0)&#xA;&#xA;&#xA;GO&#xA;/****** Object:  StoredProcedure [dbo].[usp_GenDimTime]    Script Date: 5/25/2014 3:10:03 PM ******/&#xA;SET ANSI_NULLS ON&#xA;GO&#xA;SET QUOTED_IDENTIFIER ON&#xA;GO&#xA;CREATE PROCEDURE [dbo].[usp_GenDimTime]&#xA;AS&#xA;&#xA;&#xA;DECLARE @hour as INT&#xA;DECLARE @min as INT&#xA;DECLARE @isWork AS BIt&#xA;&#xA;SET @hour = 0&#xA;&#xA;WHILE @hour &lt; 24&#xA;BEGIN&#xA;&#x9;SET @min = 0&#xA;&#x9;WHILE @min &lt; 60&#xA;&#x9;BEGIN&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;IF (@hour &gt;= 8 AND @hour &lt; 12) OR (@hour = 13 AND @min &gt;= 30) OR (@hour &gt;= 14 AND @hour &lt;= 16) OR (@hour = 17 AND @min &lt;= 30)&#xA;&#x9;&#x9;&#x9;SET @isWork = 1&#xA;&#x9;&#x9;ELSE SET @isWork = 0&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;INSERT INTO DimTime([TimeKey],[Hour],[Min],[IsWorking])&#xA;&#x9;&#x9;VALUES((@hour + 1) * 100 + @min + 1,@hour,@min,@isWork)&#xA;&#x9;&#x9;SET @min = @min + 1&#xA;&#x9;END&#xA;&#x9;&#xA;&#x9;SET @hour = @hour + 1&#xA;END&#xA;&#xA;INSERT INTO DimTime([TimeKey],[Hour],[Min],[IsWorking])&#xA;&#x9;&#x9;VALUES(100,-1,-1,0)&#xA;&#xA;GO&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
